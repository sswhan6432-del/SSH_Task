{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index" }
  ],
  "routes": [
    { "src": "/", "dest": "/website/router.html" },
    { "src": "/(.*\\.(html|css|js|png|jpg|svg|ico|woff2?))", "dest": "/website/$1" }
  ]
}
